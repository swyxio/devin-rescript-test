rescript = 1
rule astj
  command = /home/ubuntu/.opam/4.14.1/bin/bsc.exe  -w a -bs-v 11.1.0-rc.2 -uncurried -bs-no-version-header -bs-super-errors -absname -bs-ast -o $out $in
build src/jest.ast : astj ../../src/jest.res
rule deps
  command = /home/ubuntu/.opam/4.14.1/bin/bsb_helper.exe -hash 81b1b54be979df8fb025dc2929a51135 $in
  restat = 1
build src/jest.d : deps src/jest.ast src/jest.iast
build src/jest.iast : astj ../../src/jest.resi
rule mi
  command = /home/ubuntu/.opam/4.14.1/bin/bsc.exe -I src -bs-no-version-header -bs-super-errors  -w a -uncurried $in
  restat = 1
build src/jest.cmi : mi src/jest.iast
rule mj
  command = /home/ubuntu/.opam/4.14.1/bin/bsc.exe -bs-read-cmi -I src -bs-no-version-header -bs-super-errors  -w a -uncurried -bs-package-name @glennsl/rescript-jest -bs-package-output commonjs:$in_d:.bs.js $in
  restat = 1
build src/jest.cmj src/jest.bs.js : mj src/jest.ast src/jest.cmi
